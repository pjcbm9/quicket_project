<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="20dp"
    android:id="@+id/TicketBackground"
    tools:context="pjcbm9.quicket.ticketView"
    android:background="@drawable/plainbackground2">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="NAME"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketNameLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketnametv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="STATUS"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketStatusLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketstatustv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="COUNTDOWN"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketTimerLabel" />
        <TextView
            android:text="@string/hello_world"
            android:padding="10dp"
            android:gravity="center_vertical"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/tickettimertv" />
    </LinearLayout>


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="DATE"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketDateLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:textSize="12sp"
            android:layout_weight="3"
            android:layout_width="173dp"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketdatetv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="ASSIGNEE"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketAssigneeLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketassigneetv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="TYPE"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketTypeLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/tickettypetv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="LOCATION"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketLocationLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketlocationtv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="40dp">
        <TextView
            android:text="CUSTOMER"
            android:shadowColor="#ff000000"
            android:textColor="#ffefffee"
            android:shadowDx="0.0"
            android:shadowDy="0.0"
            android:shadowRadius="30"
            android:padding="10dp"
            android:layout_width="100dp"
            android:textSize="13sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/active_label"
            android:id="@+id/TicketCustomerLabel" />
        <TextView
            android:text="@string/hello_world"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:layout_weight="3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/text_box"
            android:id="@+id/ticketcustomertv" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
    <TextView
        android:text="DESCRIPTION"
        android:layout_width="fill_parent"
        android:shadowColor="#ff000000"
        android:textColor="#ffefffee"
        android:shadowDx="0.0"
        android:shadowDy="0.0"
        android:shadowRadius="30"
        android:padding="10dp"
        android:textSize="13sp"
        android:gravity="center"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/long_active_label"
        android:id="@+id/TicketDescriptionLabel" />
    </LinearLayout>
    <TextView
        android:text="Ticket Customer"
        android:padding="10dp"
        android:layout_width="fill_parent"
        android:textSize="15sp"
        android:gravity="center"
        android:layout_height="75dp"
        android:layout_gravity="center"
        android:background="@drawable/big_text_box"
        android:id="@+id/ticketdescriptiontv" />
    <View
        android:layout_width="5dp"
        android:layout_height="10dp"></View>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <Button
            android:text="DELETE"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:textSize="15sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/delete_button"
            android:id="@+id/deleteb" />
        <View
            android:layout_width="5dp"
            android:layout_height="0dp"></View>
        <Button
            android:text="EDIT"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:textSize="15sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/edit_button"
            android:id="@+id/editb" />
        <View
            android:layout_width="5dp"
            android:layout_height="0dp"></View>
        <Button
            android:text="COMPLETE"
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:textSize="15sp"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/complete_button"
            android:id="@+id/completeb" />
    </LinearLayout>
</LinearLayout>
